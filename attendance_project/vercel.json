{
    "builds": [
      {
        "src": "attendance_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.3" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "attendance_project/wsgi.py"
      }
    ]
}
